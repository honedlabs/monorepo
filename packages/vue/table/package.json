{
  "name": "@honed/table",
  "private": false,
  "version": "0.8.1",
  "license": "MIT",
  "description": "-",
  "keywords": [
    "honed",
    "table",
    "vue",
    "inertia"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run format && npm run type-check && vite build",
    "format": "npx @biomejs/biome format --write src/",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "patch": "npm version patch --no-git-tag-version",
    "minor": "npm version minor --no-git-tag-version",
    "major": "npm version major --no-git-tag-version",
    "release": "npm run build && npm publish --access public"
  },
  "author": {
    "name": "Joshua Wallace",
    "email": "josh@joshua-wallace.com",
    "url": "https://joshua-wallace.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/honedlabs/honed.git"
  },
  "homepage": "https://github.com/honedlabs/honed/tree/main/packages/vue/table#readme",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.es.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.es.js"
      }
    }
  },
  "peerDependencies": {
    "@honed/action": "^0.9.0",
    "@honed/refine": "^0.7.0",
    "@inertiajs/vue3": "^1.2.0||^2.0.0",
    "@vueuse/core": "^11.0.0||^12.0.0",
    "axios": "^1.2.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.9.3",
    "@vue/test-utils": "^2.4.6",
    "axios": "^1.2.0",
    "jsdom": "^25.0.1",
    "typescript": "~5.4.0",
    "vite": "^6.2.1",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  }
}
