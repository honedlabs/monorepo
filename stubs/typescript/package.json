{
  "name": "@:vendor_name/:package_name",
  "private": false,
  "version": "0.1.0",
  "license": "MIT",
  "description": "-",
  "keywords": [
    ":vendor_slug",
    ":package_slug",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run format && vite build",
    "format": "npx @biomejs/biome format --write src/",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "patch": "npm version patch --no-git-tag-version",
    "minor": "npm version minor --no-git-tag-version",
    "major": "npm version major --no-git-tag-version",
    "release": "npm run build && npm publish --access public"
  },
  "author": {
    "name": ":author_name",
    "email": ":author_email",
    "url": ":author_homepage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/:github_organisation/:package_name.git"
  },
  "homepage": "https://github.com/:github_organisation/:package_name#readme",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.js"
      }
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.9.3",
    "typescript": "~5.4.0",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.5"
  }
}